{
  "buildCommand": "pnpm turbo prune front --docker && cd out && pnpm install --frozen-lockfile && pnpm turbo build --filter=front",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "outputDirectory": "out/apps/front/dist/front/browser",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "apps/front/dist/front/browser/**": {
      "maxDuration": 30
    }
  }
}

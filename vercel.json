{
  "buildCommand": "ng build",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "outputDirectory": "dist/front/browser",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "apps/front/dist/front/browser/**": {
      "maxDuration": 30
    }
  }
}
